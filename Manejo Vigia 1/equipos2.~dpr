program equipos2;

uses
  Forms,
  Pdatos in 'Pdatos.pas' {Datos: TDataModule},
  Ventana in 'Ventana.pas' {FEquipos};

{$R *.RES}

begin
  Application.Initialize;
  Application.Title := 'Administrador de Equipos Vigía 2.0';
  Application.CreateForm(TDatos, Datos);
  Application.CreateForm(TFEquipos, FEquipos);
  Application.Run;
end.
